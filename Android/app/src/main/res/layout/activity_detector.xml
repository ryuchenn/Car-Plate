<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DetectorActivity">


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Carplate"
        tools:layout_editor_absoluteX="455dp"
        tools:layout_editor_absoluteY="701dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Notifacation to open entry"
        tools:layout_editor_absoluteX="1030dp"
        tools:layout_editor_absoluteY="701dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Time"
        tools:layout_editor_absoluteX="1103dp"
        tools:layout_editor_absoluteY="33dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EntryTime"
        tools:layout_editor_absoluteX="1103dp"
        tools:layout_editor_absoluteY="136dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="$5.00/hr\n Max:$30/time"
        tools:layout_editor_absoluteX="1103dp"
        tools:layout_editor_absoluteY="345dp" />

    <androidx.camera.view.PreviewView
        android:id="@+id/camera_preview"
        android:layout_width="985dp"
        android:layout_height="622dp"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="-1dp" />
</androidx.constraintlayout.widget.ConstraintLayout>

